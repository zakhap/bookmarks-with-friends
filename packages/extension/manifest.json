{
  "manifest_version": 3,
  "name": "Bookmarks with Friends",
  "version": "0.1.0",
  "description": "Save and share bookmarks with your friends",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.svg",
      "48": "icon48.svg",
      "128": "icon128.svg"
    }
  },
  "icons": {
    "16": "icon16.svg",
    "48": "icon48.svg",
    "128": "icon128.svg"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}
